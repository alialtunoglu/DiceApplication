#include <main.h>
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int sayi;

int dizi[7]={0x00,0x08,0x22,0x2a,0x63,0x6b,0x77};
int  main(void)
{
   set_tris_c(0x00);
   set_tris_d(0x00);
   output_c(0x00);
   output_d(0x00);
   
  // srand(15);
   sayi=1+rand()%6;
  // sayi=5;
  
   output_c(dizi[0]);
   output_d(dizi[0]);
   
   return 0;
}
